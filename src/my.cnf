[client]
socket = "/data1/mysql33113/mysql.sock"

[mysql]
default-character-set = "utf8mb4"
auto-rehash

[mysqld33113]
mysqld = "/usr/bin/msqld"
port = "33113"
log_timestamps = "SYSTEM"
basedir = "/data1/mysql"
datadir = "/data1/mysql33113/data"
tmpdir = "/data1/mysql33113/data"
socket = "/data1/mysql33113/mysql.sock"
log_error = "/data1/mysql33113/data/mysqld.err"
pid_file = "/data1/mysql33113/mysql.pid"
server-id = "33113149"
transaction-isolation = "READ-COMMITTED"
character_set_server = "utf8mb4"
open_files_limit = "65535"
lower_case_table_names = "1"
max_connections = "200"
max_connect_errors = "100000000"
connect_timeout = "10"
lock_wait_timeout = "3600"
wait_timeout = "86400"
interactive_timeout = "86400"
max_allowed_packet = "64M"
thread_cache_size = "512"
sql_mode = "ONLY_FULL_GROUP_BY,STRICT_TRANS_TABLES,NO_ZERO_IN_DATE,NO_ZERO_DATE,ERROR_FOR_DIVISION_BY_ZERO,NO_AUTO_CREATE_USER,NO_ENGINE_SUBSTITUTION,NO_AUTO_VALUE_ON_ZERO"
binlog_format = "row"
binlog_row_image = "full"
log-bin = "/data1/mysql33113/binlog/mysql-bin"
max_binlog_size = "1G"
expire_logs_days = "7"
binlog_error_action = "ABORT_SERVER"
log_slave_updates = "1"
relay_log = "/data1/mysql33113/binlog/mysql-relay"
max_relay_log_size = "1G"
relay_log_purge = "0"
relay_log_recovery = "1"
master_info_repository = "TABLE"
relay_log_info_repository = "TABLE"
report_host = "192.168.171.149"
report_port = "33113"
sync_binlog = "1"
innodb_flush_log_at_trx_commit = "1"
innodb_buffer_pool_size = "1024M"
innodb_sort_buffer_size = "4M"
innodb_log_buffer_size = "32M"
innodb_log_file_size = "1G"
innodb_log_files_in_group = "4"
innodb_lock_wait_timeout = "60"
innodb_lock_group_home_dir = "/data1/mysql33113/data"
innodb_io_capacity = "1000"
innodb_io_capacity_max = "2000"
innodb_file_per_table = "1"
innodb_stats_persistent_sample_pages = "64"
innodb_online_alter_log_max_size = "1G"
innodb_thread_concurrency = "0"
innodb_write_io_threads = "16"
innodb_read_io_threads = "16"
innodb_page_cleaners = "16"
innodb_flush_method = "O_DIRECT"
innodb_monitor_enable = "all"
innodb_print_all_deadlocks = "1"
gitd_mode = "on"
enforce_gtid_consistency = "1"
binlog_gtid_simple_recovery = "1"
slave-parallel-type = "LOGICAL_CLOCK"
slave-parallel-workers = "16"
slave_preserve_commit_order = "1"
slave_transaction_retries = "128"
loose_innodb_numa_interleave = "1"
innodb_buffer_pool_dump_pct = "40"
innodb_undo_directory = "/data1/mysql33113/binlog/"
innodb_undo_logs = "128"
innodb_undo_tablespaces = "0"
innodb_undo_log_truncate = "1"
innodb_max_undo_log_size = "2G"
InnodbPurgeRsegTruncateFrequency = "128"
table_open_cache = "2048"
tmp_table_size = "64M"
max_heap_table_size = "64M"
sort_buffer_size = "4M"
join_buffer_size = "4M"
read_buffer_size = "8M"
read_rnd_buffer_size = "4M"
key_buffer_size = "32M"
bulk_insert_buffer_size = "64M"
binlog_cache_size = "1M"
slow_query_log_file = "/data1/mysql33113/data/mysql-slow.log"
slow_query_log = "ON"
long_query_time = "1"
log_output = "file"
log_slow_admin_statements = "1"
log_slave_slave_statements = "1"
log_throttle_queries_not_using_indexes = "10"
performance_schema = "ON"
performance-schema-instrument = "wait/lock/metadata/sql/dml/memory=ON"
plugin-load-add = "validate_password.so"
validate-password = "FORCE_PLUS_PERMANENT"
validate_password_policy = "MEDIUM"
symbolic-links = "0"
secure_file_priv=
skip-host-cache
skip-name-resolve